    sas.standAlonePassback = function(config){
        var self = this;
        self.d = document;
        self.w = window;
        self.c = config;
        self.logOn = false;

        self.addElement = function (type, parent, attr){
            self.log("addElement", type, parent, attr);

            var el = self.d.createElement(type);
            for (var i in attr)
            el.setAttribute(i, attr[i]);
            parent.appendChild(el);
            return el;
        };

        self.log = function(){
            if(self.logOn) console.log(arguments);
        };

        self.addCss = function (){
            self.log("addCss");

            var css = self.d.createElement('style');
            css.type = 'text/css';

            var style = [
                '#'+self.c.tagId+'{margin:0;padding:0;display:block;}',
                '#sas_iframe_'+self.c.insertionId+'{width:'+self.c.creative.width+'px;height:'+self.c.creative.height+'px;margin:0 auto;padding:0;display:block;}',
            ].join('\r\n');

            if (css.styleSheet) {
                css.styleSheet.cssText = style;
            } else {
                css.appendChild(self.d.createTextNode(style));
            }
            self.d.getElementsByTagName('head')[0].appendChild(css);
        };

        self.addCreative = function(creative, parent){
            self.log("addCreative", creative, parent);

            if(0 != self.c.creative.type) return;

            if(self.c.isAsync){
                var ifr = self.addElement('iframe', parent, {id: 'sas_iframe_'+self.c.insertionId, src: 'about:blank', width: self.c.creative.width+'px', height: self.c.creative.height+'px', frameBorder: '0', 'scrolling': 'no', marginheight: '0', marginwidth: '0'});
