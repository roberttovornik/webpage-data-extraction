                d.write('<!DOCTYPE html><head></head><body style="padding:0;margin:0 auto">' + self.c.creative.agencyCode + '</body></html>');
                d.close();
            } else self.d.write(self.c.creative.agencyCode);
        };

        self.init = function(){
            self.log("init");

            //iframe out, ifriendly
            if (typeof(inDapIF) != 'undefined' && inDapIF) {
                //set async mode
                self.c.isAsync = true;

                self.d = window.parent.document;
                self.w = window.parent;
            }

            //place for ad
            self.adPlacement = self.d.getElementById(self.c.tagId);

            //fix for the friendly iframe
            if(!self.adPlacement && typeof(inDapIF) != 'undefined' && inDapIF) {
                var iframe = window.frameElement;
                self.adPlacement = self.d.createElement('DIV');
                self.adPlacement.setAttribute('id', self.c.tagId);
                iframe.parentNode.insertBefore(self.adPlacement, iframe); 
            }

            //load css
            self.addCss();

            //add creative
            self.addCreative(self.c.creative, self.adPlacement);
        };
    };
